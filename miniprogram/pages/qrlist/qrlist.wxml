<!--miniprogram/pages/qrlist/qrlist.wxml-->
<navbar>社区</navbar>
<contain>
  <view class="list">
    <view class="item" bindtap="goqrcode" wx:for="{{list}}" wx:key="{{index}}">
      <styleitem cover="{{item.cover}}" avatar="{{item._author_avatar}}" username="{{item._author_name}}" tit="{{item.element_name}}" num="{{item._like}}"></styleitem>
    </view>
  </view>
  <page></page>
</contain>