/* components/missbox/missbox.wxss */

.miss-box{
  text-align: center;
  color: #fff;
}

.miss {
  width: 100%;
  height: 350rpx;
  box-sizing: border-box;
  margin: 100rpx auto;
  display: inline-flex;
  align-items: center;
  justify-content: center;
}
.tit{
  font-size: 36rpx;
  font-weight: bold;
  margin-bottom: 100rpx;
}
.miss-nodata{
  width: 100%;
  height: 100%;
  background-image: url("data:image/png;base64,");
  background-size: contain;
  background-position: center center;
  background-repeat: no-repeat;
}
.miss-err{
  width: 100%;
  height: 100%;
  background-image: url("data:image/png;base64,");
  background-size: contain;
  background-position: center center;
  background-repeat: no-repeat;
}

.miss-loading {
  margin: 0 auto;
  width: 40rpx !important;
  height: 40rpx !important;
  border-radius: 40rpx;
  background-color: #FFE142;
  position: relative;
  animation: dot 1s linear infinite forwards;
  animation-delay: .2s;
}

.miss-loading::after {
  position: absolute;
  content: '';
  width: 40rpx;
  height: 40rpx;
  border-radius: 40rpx;
  background-color: #FFE142;
  left: 100rpx;
  animation: dot 1s linear infinite;

}

.miss-loading::before {
  position: absolute;
  content: '';
  width: 40rpx;
  height: 40rpx;
  border-radius: 40rpx;
  background-color: #FFE142;
  right: 100rpx;
  animation: dot 1s linear infinite forwards;
  animation-delay: .4s;

}

@keyframes dot {
  from {
      background-color: transparent;
  }

  to {
      background-color: #FFE142;
  }
}