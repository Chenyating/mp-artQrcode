/* components/icon/icon.wxss */

.icon {
  width: 80rpx;
  height: 80rpx;
  border-radius: 10rpx;
  border: 4rpx solid #000000;
  box-sizing: border-box;
  box-shadow: 4rpx 4rpx 0rpx #D098FF;
  background: #fff;
  margin: 15rpx;
}
.round{
  border-radius: 100%;
}
.icon-add{
  background-image: url("data:image/png;base64,");
  background-size: 43rpx 43rpx;
