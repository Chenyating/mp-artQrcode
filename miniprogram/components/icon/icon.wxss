.icon {
  width: 80rpx;
  height: 80rpx;
  box-sizing: border-box;
}

.border {
  border-radius: 10rpx;
  margin: 15rpx;
  border: 4rpx solid #000000;
  box-shadow: var(--box-shadow);
  background: #fff;
}

.round {
  border-radius: 100%;
}

